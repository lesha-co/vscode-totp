{
  "name": "totp",
  "displayName": "OTP Generator",
  "description": "Generate OTP easily ",
  "version": "2.3.2",
  "publisher": "lesha",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "totp.pick",
        "title": "Get an one-time password"
      },
      {
        "command": "totp.new",
        "title": "Add new one-time password"
      },
      {
        "command": "totp.backup",
        "title": "Backup seed database"
      },
      {
        "command": "totp.restore",
        "title": "Restore seed database from backup"
      },
      {
        "command": "totp.edit",
        "title": "Edit OTP passwords"
      },
      {
        "command": "totp.clear",
        "title": "Purge all codes"
      }
    ],
    "keybindings": {
      "command": "totp.pick",
      "key": "shift+alt+O"
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lesha-co/vscode-totp.git"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  },
  "dependencies": {
    "simple-totp": "^1.0.5"
  }
}
