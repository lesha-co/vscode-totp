{
  "name": "totp",
  "displayName": "OTP Generator",
  "description": "Generate OTP easily ",
  "version": "2.3.2",
  "publisher": "lesha",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:totp.pick"
  ],
  "contributes": {
    "commands": [
      {
        "command": "totp.pick",
        "title": "Get an one-time password"
      },
      {
        "command": "totp.new",
        "title": "Add new one-time password"
      },
      {
        "command": "totp.backup",
        "title": "Backup seed database"
      },
      {
        "command": "totp.restore",
        "title": "Restore seed database from backup"
      },
      {
        "command": "totp.edit",
        "title": "Edit OTP passwords"
      },
      {
        "command": "totp.clear",
        "title": "Purge all codes"
      }
    ],
    "keybindings": {
      "command": "totp.pick",
      "key": "shift+alt+O"
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "compile": "tsc -p ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lesha-co/vscode-totp.git"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.74.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5",
    "vsce": "^2.15.0",
    "vscode-test": "^1.6.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "simple-totp": "^1.4.2"
  }
}
